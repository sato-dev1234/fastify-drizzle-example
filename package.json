{
  "name": "fastify-drizzle-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tspc -p tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --files -r tsconfig-paths/register --watch -- src/server",
    "test": "jest",
    "test:e2e": "cross-env NODE_ENV=e2e_test jest --detectOpenHandles --config=./test/jest-e2e.config.json",
    "test:cov": "jest --collectCoverage=true",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\"  --fix",
    "drizzle:generate": "drizzle-kit generate:pg --config=./drizzle/default/drizzle.config.ts",
    "drizzle:push": "cross-env NODE_ENV=development drizzle-kit push:pg --config=./drizzle/default/drizzle.config.ts",
    "drizzle:studio": "cross-env NODE_ENV=development drizzle-kit studio --config=./drizzle/default/drizzle.config.ts",
    "drizzle:generate:e2e": "drizzle-kit generate:pg --config=./drizzle/e2e_test/drizzle.e2e.config.ts",
    "drizzle:push:e2e": "cross-env NODE_ENV=e2e_test drizzle-kit push:pg --config=./drizzle/e2e_test/drizzle.e2e.config.ts",
    "drizzle:studio:e2e": "cross-env NODE_ENV=e2e_test drizzle-kit studio --config=./drizzle/e2e_test/drizzle.e2e.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/ajv-compiler": "^3.5.0",
    "@fastify/env": "^4.3.0",
    "@fastify/error": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "ajv": "^8.12.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "fastify-zod": "^1.4.0",
    "i18next": "^23.7.16",
    "inversify": "^6.0.2",
    "libphonenumber-js": "^1.10.53",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "zod": "^3.22.4",
    "zod-i18n-map": "^2.25.0"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.6.8",
    "@swc-node/register": "^1.6.8",
    "@swc/helpers": "^0.5.3",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typescript-transform-paths": "^3.4.6"
  }
}
